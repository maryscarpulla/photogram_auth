<div class="page-header">
  <h1>
    Mylikes
    <a href="/mylikes/new" class="btn btn-lg btn-success">New Mylike</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Photo</th>
        <th>Actions</th>
      </tr>

      <% @mylikes.each do |mylike| %>
      <tr>
        <td><%= mylike.user_id %></td>
        <td><%= mylike.photo_id %></td>
        <td>
          <a href="/mylikes/<%= mylike.id %>" class="btn btn-primary">Show</a>
          <a href="/mylikes/<%= mylike.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_mylike/<%= mylike.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
